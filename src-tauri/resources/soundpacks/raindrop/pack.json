{
  "id": "raindrop",
  "name": "Raindrop",
  "author": "KeySound",
  "version": "1.0.0",
  "description": "Gentle water droplets on a calm surface",

  "defaults": {
    "keydown": "sounds/keydown.wav",
    "volume": 0.8
  },

  "key_overrides": {
    "Space": { "keydown": "sounds/keydown-space.wav", "volume": 1.0 },
    "Return": { "keydown": "sounds/keydown-enter.wav", "volume": 1.0 }
  },

  "category_overrides": {
    "modifiers": {
      "keys": [
        "ShiftLeft",
        "ShiftRight",
        "ControlLeft",
        "ControlRight",
        "Alt",
        "AltGr",
        "MetaLeft",
        "MetaRight"
      ],
      "keydown": "sounds/keydown-modifier.wav",
      "volume": 0.6
    },
    "delete": {
      "keys": ["Backspace", "Delete"],
      "keydown": "sounds/keydown-backspace.wav"
    }
  }
}

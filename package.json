{
  "name": "keysound",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tsx scripts/run-tauri.ts",
    "tauri:dev": "tsx scripts/run-tauri.ts dev",
    "tauri:build": "tsx scripts/run-tauri.ts build --target x86_64-pc-windows-gnu",
    "lint": "eslint . && node scripts/with-path.mjs cargo clippy --manifest-path src-tauri/Cargo.toml -- -D warnings",
    "lint:fix": "eslint . --fix",
    "format": "oxfmt --write . && node scripts/with-path.mjs cargo fmt --manifest-path src-tauri/Cargo.toml",
    "format:check": "oxfmt --check . && node scripts/with-path.mjs cargo fmt --manifest-path src-tauri/Cargo.toml --check"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-autostart": "^2.5.1",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-store": "^2.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tauri-apps/cli": "^2",
    "audio-decode": "^2.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.14.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "oxfmt": "^0.28.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.0.3"
  }
}
